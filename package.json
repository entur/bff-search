{
    "name": "@entur/bff-search",
    "version": "1.0.0",
    "description": "A backend that handles travel search for Entur clients.",
    "author": "Entur AS",
    "license": "EUPL-1.2",
    "private": true,
    "engines": {
        "node": ">=14"
    },
    "scripts": {
        "start": "node dist/server.js",
        "test": "jest",
        "lint": "eslint . --ext .js,.jsx,.ts,.tsx && npm run format:check",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "debug": "./scripts/debug.sh",
        "dev": "npm run debug dev",
        "staging": "npm run debug staging",
        "prod": "npm run debug prod",
        "build:dev": "tsc && npm run populate-env-vars dev",
        "build:staging": "tsc && npm run populate-env-vars staging",
        "build:prod": "tsc && npm run populate-env-vars prod",
        "build:beta": "tsc && npm run populate-env-vars beta",
        "deploy": "./scripts/deploy.sh",
        "apigee": "./scripts/deploy-apigee.sh",
        "transpile": "tsc",
        "populate-env-vars": "ts-node ./scripts/environment.ts"
    },
    "dependencies": {
        "@entur/sdk": "^4.3.0",
        "@google-cloud/bigquery": "^5.6.0",
        "@google-cloud/logging-winston": "^4.0.4",
        "@google-cloud/trace-agent": "^5.1.3",
        "@turf/boolean-contains": "^6.3.0",
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "date-fns": "^2.21.3",
        "express": "^4.17.1",
        "express-jwt": "^6.0.0",
        "haversine-distance": "^1.2.1",
        "jwks-rsa": "^1.12.3",
        "lz-string": "^1.4.4",
        "node-fetch": "^2.6.1",
        "redis": "^3.1.2",
        "timezone-support": "^2.0.2",
        "uuid": "^7.0.3",
        "winston": "^3.3.3"
    },
    "devDependencies": {
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/geojson": "^7946.0.8",
        "@types/jest": "^27.0.2",
        "@types/lz-string": "^1.3.34",
        "@types/node": "^16.11.6",
        "@types/node-fetch": "^2.5.12",
        "@types/redis": "^2.8.32",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/eslint-plugin": "^5.3.0",
        "@typescript-eslint/parser": "^5.3.0",
        "apigeetool": "^0.15.1",
        "eslint": "^8.1.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jest": "^27.3.1",
        "nodemon": "^2.0.14",
        "prettier": "2.4.1",
        "ts-jest": "^27.0.7",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    }
}
