{
    "name": "@entur/bff-search",
    "version": "1.0.0",
    "description": "A backend that handles travel search for Entur clients.",
    "author": "Entur AS",
    "license": "EUPL-1.2",
    "private": true,
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "start": "node dist/server.js",
        "test": "jest && npm run unused-exports",
        "lint": "npm run eslint && npm run format:check",
        "lintfix": "npm run eslint --fix && npm run format",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "ts": "tsc --noEmit && tsc -p scripts/tsconfig.json",
        "debug": "./scripts/debug.sh",
        "build": "./scripts/build.sh",
        "apigee": "./scripts/deploy-apigee.sh",
        "slack": "./scripts/slack.sh",
        "transpile": "tsc",
        "populate-env-vars": "ts-node ./scripts/environment.ts",
        "unused-exports": "node --require ts-node/register/transpile-only scripts/unused-exports.ts",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^6.2.1",
        "@google-cloud/logging-winston": "^5.3.0",
        "@google-cloud/storage": "^6.12.0",
        "@google-cloud/trace-agent": "^5.1.6",
        "body-parser": "^1.20.2",
        "clean-deep": "^3.4.0",
        "cors": "^2.8.5",
        "date-fns": "^2.30.0",
        "express": "^4.18.2",
        "express-jwt": "^6.1.2",
        "express-unless": "^2.1.3",
        "google-auth-library": "^8.9.0",
        "graphql-request": "^4.3.0",
        "jwks-rsa": "^2.1.5",
        "lz-string": "^1.5.0",
        "node-fetch": "^2.7.0",
        "redis": "^3.1.2",
        "semver": "^7.5.4",
        "timezone-support": "^2.2.0",
        "uuid": "^8.3.2",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@babel/code-frame": "^7.23.5",
        "@graphql-codegen/cli": "2.16.5",
        "@graphql-codegen/typescript": "2.8.8",
        "@graphql-codegen/typescript-graphql-files-modules": "2.2.1",
        "@graphql-codegen/typescript-operations": "2.5.13",
        "@types/babel__code-frame": "^7.0.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-jwt": "^6.0.4",
        "@types/graphql": "^14.5.0",
        "@types/jest": "^27.5.2",
        "@types/lz-string": "^1.3.34",
        "@types/node": "18.19.3",
        "@types/node-fetch": "^2.6.9",
        "@types/redis": "^2.8.32",
        "@types/semver": "^7.5.6",
        "@types/uuid": "^8.3.4",
        "@typescript-eslint/eslint-plugin": "^5.62.0",
        "@typescript-eslint/parser": "^5.62.0",
        "apigeetool": "^0.16.4",
        "eslint": "^8.56.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-prettier": "^4.2.1",
        "graphql": "^16.8.1",
        "jest": "^27.5.1",
        "nodemon": "^2.0.22",
        "prettier": "2.8.8",
        "ts-jest": "^27.1.5",
        "ts-node": "^10.9.2",
        "ts-unused-exports": "^8.0.5",
        "typescript": "^4.9.5"
    }
}
