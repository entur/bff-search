{
  "name": "@entur/bff-search",
  "version": "1.0.0",
  "description": "A backend that handles travel search for Entur clients.",
  "author": "Entur AS",
  "license": "EUPL-1.2",
  "private": true,
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "./scripts/debug.sh",
    "dev": "npm run debug dev",
    "staging": "npm run debug staging",
    "prod": "npm run debug prod",
    "build:dev": "tsc && npm run populate-env-vars dev",
    "build:staging": "tsc && npm run populate-env-vars staging",
    "build:prod": "tsc && npm run populate-env-vars prod",
    "build:beta": "tsc && npm run populate-env-vars beta",
    "deploy": "./scripts/deploy.sh",
    "apigee": "./scripts/deploy-apigee.sh",
    "transpile": "tsc",
    "populate-env-vars": "ts-node ./scripts/environment.ts"
  },
  "dependencies": {
    "@entur/sdk": "^3.5.0",
    "@google-cloud/bigquery": "^5.6.0",
    "@google-cloud/logging-winston": "^4.0.4",
    "@google-cloud/trace-agent": "^5.1.3",
    "@turf/boolean-contains": "^6.3.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.21.3",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "haversine-distance": "^1.2.1",
    "jwks-rsa": "^1.12.3",
    "lz-string": "^1.4.4",
    "node-fetch": "^2.6.1",
    "redis": "^3.1.2",
    "timezone-support": "^2.0.2",
    "uuid": "^7.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.23",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^12.20.12",
    "@types/node-fetch": "^2.5.10",
    "@types/redis": "^2.8.28",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "apigeetool": "^0.14.2",
    "eslint": "^7.26.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^4.2.4"
  }
