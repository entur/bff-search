{
    "name": "@entur/bff-search",
    "version": "1.0.0",
    "description": "A backend that handles travel search for Entur clients.",
    "author": "Entur AS",
    "license": "EUPL-1.2",
    "private": true,
    "engines": {
        "node": ">=18"
    },
    "scripts": {
        "start": "node dist/server.js",
        "test": "jest && npm run unused-exports",
        "lint": "npm run eslint && npm run format:check",
        "lintfix": "npm run eslint --fix && npm run format",
        "format": "prettier . --write",
        "format:check": "prettier . --check",
        "eslint": "eslint . --ext .js,.jsx,.ts,.tsx",
        "ts": "tsc --noEmit && tsc -p scripts/tsconfig.json",
        "debug": "./scripts/debug.sh",
        "build": "./scripts/build.sh",
        "apigee": "./scripts/deploy-apigee.sh",
        "slack": "./scripts/slack.sh",
        "transpile": "tsc",
        "populate-env-vars": "ts-node ./scripts/environment.ts",
        "unused-exports": "node --require ts-node/register/transpile-only scripts/unused-exports.ts",
        "codegen": "graphql-codegen --config codegen.yml"
    },
    "dependencies": {
        "@google-cloud/bigquery": "^7.3.0",
        "@google-cloud/logging-winston": "^6.0.0",
        "@google-cloud/storage": "^7.7.0",
        "@google-cloud/trace-agent": "^7.1.2",
        "body-parser": "^1.20.2",
        "clean-deep": "^3.4.0",
        "cors": "^2.8.5",
        "date-fns": "^3.1.0",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "express-unless": "^2.1.3",
        "google-auth-library": "^9.4.1",
        "graphql-request": "^6.1.0",
        "jwks-rsa": "^3.1.0",
        "lz-string": "^1.5.0",
        "node-fetch": "^2.7.0",
        "redis": "^4.6.12",
        "semver": "^7.5.4",
        "timezone-support": "^3.1.0",
        "uuid": "^9.0.1",
        "winston": "^3.11.0"
    },
    "devDependencies": {
        "@babel/code-frame": "^7.23.5",
        "@graphql-codegen/cli": "5.0.0",
        "@graphql-codegen/typescript": "4.0.1",
        "@graphql-codegen/typescript-graphql-files-modules": "3.0.0",
        "@graphql-codegen/typescript-operations": "4.0.1",
        "@types/babel__code-frame": "^7.0.6",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/express-jwt": "^7.4.2",
        "@types/jest": "^29.5.11",
        "@types/lz-string": "^1.5.0",
        "@types/node": "18.19.4",
        "@types/node-fetch": "^2.6.10",
        "@types/semver": "^7.5.6",
        "@types/uuid": "^9.0.7",
        "@typescript-eslint/eslint-plugin": "^6.17.0",
        "@typescript-eslint/parser": "^6.17.0",
        "apigeetool": "^0.16.4",
        "eslint": "^8.56.0",
        "eslint-plugin-fp": "^2.3.0",
        "eslint-plugin-prettier": "^5.1.2",
        "graphql": "^16.8.1",
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "prettier": "3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.2",
        "ts-unused-exports": "^10.0.1",
        "typescript": "^5.3.3"
    }
}
