{
  "name": "@entur/bff-search",
  "version": "1.0.0",
  "description": "A backend that handles travel search for Entur clients.",
  "author": "Entur AS",
  "license": "EUPL-1.2",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "node dist/server.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "debug": "./scripts/debug.sh",
    "dev": "npm run debug dev",
    "staging": "npm run debug staging",
    "prod": "npm run debug prod",
    "build:dev": "tsc && npm run populate-env-vars dev",
    "build:staging": "tsc && npm run populate-env-vars staging",
    "build:prod": "tsc && npm run populate-env-vars prod",
    "build:beta": "tsc && npm run populate-env-vars beta",
    "deploy": "./scripts/deploy.sh",
    "apigee": "./scripts/deploy-apigee.sh",
    "transpile": "tsc",
    "populate-env-vars": "ts-node ./scripts/environment.ts"
  },
  "dependencies": {
    "@entur/sdk": "^1.9.0",
    "@google-cloud/bigquery": "^4.7.0",
    "@google-cloud/logging-winston": "^3.0.6",
    "@google-cloud/trace-agent": "^4.2.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "date-fns": "^2.15.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "haversine-distance": "^1.2.1",
    "jwks-rsa": "^1.8.1",
    "lz-string": "^1.4.4",
    "node-fetch": "^2.6.0",
    "redis": "^3.0.2",
    "timezone-support": "^2.0.2",
    "uuid": "^7.0.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/jest": "^24.9.1",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^13.13.15",
    "@types/node-fetch": "^2.5.7",
    "@types/redis": "^2.8.25",
    "@types/uuid": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "apigeetool": "^0.14.2",
    "eslint": "^6.8.0",
    "eslint-loader": "^3.0.4",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^24.9.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ts-jest": "^24.3.0",
    "ts-node": "^8.10.2",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.7"
  }
}
