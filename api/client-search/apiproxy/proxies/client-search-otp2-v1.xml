<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<ProxyEndpoint name="client-search-otp2-v1">
  <Description></Description>
  <FaultRules/>
  <Flows/>
  <PostFlow name="PostFlow">
    <Request/>
    <Response/>
  </PostFlow>
  <PreFlow name="PreFlow">
    <Request>
      <Step>
        <FaultRules/>
        <Name>spikeArrest</Name>
      </Step>
      <Step>
        <FaultRules/>
        <Name>flowCallout.getEnvironmentConfiguration</Name>
      </Step>
    </Request>
    <Response/>
  </PreFlow>
  <HTTPProxyConnection>
    <BasePath>/client/search/otp2/v1</BasePath>
    <Properties/>
    <VirtualHost>entur</VirtualHost>
  </HTTPProxyConnection>
  <RouteRule name="client-search-otp2-v1">
    <TargetEndpoint>client-search-otp2-v1</TargetEndpoint>
  </RouteRule>
</ProxyEndpoint>
